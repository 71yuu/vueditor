{
  "name": "vueditor",
  "version": "0.4.0",
  "description": "a wysiwyg editor based on Vue.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hifarer/vueditor.git"
  },
  "main": "dist/vueditor.min.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --open --config ./build/webpack.dev.config.js --progress --hide-modules",
    "build": "webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "eslint": "eslint --fix --ext .vue,.js ./src/"
  },
  "dependencies": {
    "vue": ">=2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
    "url-loader": "^2.0.0",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  },
  "keywords": [
    "vue editor",
    "vue.js editor",
    "rich text editor",
    "wysiwyg editor",
    "vue.js wysiwyg editor"
  ],
  "author": "hifarer",
  "license": "MIT"
}
